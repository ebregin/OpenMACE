{
  "name": "mace-gui",
  "productName": "MACE_GUI",
  "version": "1.0.0",
  "description": "MACE GUI for development purposes",
  "main": "main.js",
  "config": {
    "port": "8082"
  },
  "scripts": {
    "version": "tsc -v",
    "start": "cross-env NODE_ENV=development PORT=$npm_package_config_port electron .",
    "build": "./node_modules/.bin/webpack",
    "watch": "cross-env PORT=$npm_package_config_port ./node_modules/.bin/webpack-dev-server --hot --inline",
    "build:prod": "cross-env PORT=$npm_package_config_port webpack --env.prod -p",
    "launch": "cross-env PORT=$npm_package_config_port parallel \"watch --mace-gui:port=$npm_package_config_port\" \"start --mace-gui:port=$npm_package_config_port\"",
    "build:launch": "cross-env PORT=$npm_package_config_port parallel \"build:prod --mace-gui:port=$npm_package_config_port\" \"launch --mace-gui:port=$npm_package_config_port\"",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "build:release": "electron-packager . MACE_GUI --overwrite --all --prune=true --out=release-builds"
  },
  "author": "Scotch",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@turf/turf": "5.1.6",
    "babel-loader": "8.0.6",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "babelify": "10.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "3.1.0",
    "deepcopy": "0.6.3",
    "electron-prebuilt": "1.4.13",
    "electron-reload": "1.4.1",
    "file-loader": "4.1.0",
    "heatmap.js": "2.0.5",
    "leaflet": "1.5.1",
    "leaflet-draw": "1.0.4",
    "leaflet-geometryutil": "0.9.1",
    "material-ui": "0.20.2",
    "net": "1.0.2",
    "react": "16.8.6",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.8.6",
    "react-leaflet": "2.4.0",
    "react-leaflet-draw": "0.19.0",
    "react-leaflet-heatmap-layer": "2.0.0",
    "react-notification-system": "0.2.17",
    "react-tap-event-plugin": "3.0.3",
    "style-loader": "0.23.1",
    "ts-loader": "6.0.4",
    "typescript": "^3.5.3",
    "url-loader": "2.1.0",
    "webpack": "4.36.1"
  },
  "devDependencies": {
    "@types/material-ui": "0.21.6",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-bootstrap": "0.32.19",
    "@types/react-dom": "16.8.4",
    "@types/react-leaflet": "2.2.1",
    "@types/react-notification-system": "0.2.39",
    "browserify": "16.3.0",
    "electron-packager": "^14.0.2",
    "electron-rebuild": "1.8.5",
    "npm-parallel": "^2.0.1",
    "react-addons-perf": "15.4.2",
    "tsify": "4.0.1",
    "tslint": "^5.18.0",
    "watchify": "3.11.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": ">=3.7.2"
  }
}
